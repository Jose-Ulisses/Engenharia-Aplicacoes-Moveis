<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acessogeodb">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AcessoGeoDB">

        <activity
            android:name=".TalhaoActivitys.AdicionarTalhaoActivity"
            android:exported="false"
            android:parentActivityName=".ColheitaActivitys.Lavouras.LavouraActivity" />

        <activity
            android:name=".ColheitaActivitys.Lavouras.TodasLavourasActivity"
            android:exported="false"
            android:parentActivityName=".ColheitaActivitys.Lavouras.LavouraActivity" />

        <activity
            android:name=".ColheitaActivitys.Lavouras.AdicionarLavouraActivity"
            android:exported="false"
            android:parentActivityName=".ColheitaActivitys.Lavouras.LavouraActivity" />

        <activity
            android:name=".ColheitaActivitys.Lavouras.LavouraActivity"
            android:exported="false"
            android:parentActivityName=".ColheitaActivitys.ColheitaActivity" />

        <activity
            android:name=".ColheitaActivitys.ColheitaActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".PanhadoresActivitys.InformacoesPanhadorActivity"
            android:exported="false"
            android:parentActivityName=".PanhadoresActivitys.TodosPanhadoresActivity" />

        <activity
            android:name=".PanhadoresActivitys.TodosPanhadoresActivity"
            android:exported="false"
            android:parentActivityName=".PanhadoresActivitys.PanhadoresActivity" />

        <activity
            android:name=".PanhadoresActivitys.AdicionarPanhadoresActivity"
            android:exported="false"
            android:parentActivityName=".PanhadoresActivitys.PanhadoresActivity" />

        <activity
            android:name=".PanhadoresActivitys.PanhadoresActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>